public class Solution_2_3 {
    public static void main(String[] args) {

        double x1 = 0.0, x2 = 0.0, x3 = 0.0, y1 = 0.0, y2 = 0.0, y3 = 0.0;
        boolean isLine = false;
		
        if (x3==(x2-x1)+x2 && y3==(y2-y1)+y2) {
            isLine = true;
        }
        System.out.println(isLine);
	  }
}
